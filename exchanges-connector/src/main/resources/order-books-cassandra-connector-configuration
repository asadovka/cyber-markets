name=OrderBooksRawDataCassandraConnector
connector.class=com.datamountaineer.streamreactor.connect.cassandra.sink.CassandraSinkConnector
connect.cassandra.key.space=markets
topics=ORDERBOOKS-BITFINEX,ORDERBOOKS-BITSTAMP,ORDERBOOKS-GDAX,ORDERBOOKS-HITBTC,ORDERBOOKS-POLONIEX,ORDERBOOKS-ETHERDELTA
tasks.max=6
connect.cassandra.kcql=INSERT INTO orderbook SELECT * FROM ORDERBOOKS-BITFINEX;INSERT INTO orderbook SELECT * FROM ORDERBOOKS-BITSTAMP;INSERT INTO orderbook SELECT * FROM ORDERBOOKS-GDAX;INSERT INTO orderbook SELECT * FROM ORDERBOOKS-HITBTC;INSERT INTO orderbook SELECT * FROM ORDERBOOKS-POLONIEX;INSERT INTO orderbook SELECT * FROM ORDERBOOKS-ETHERDELTA;
connect.cassandra.fetch.size=5000
connect.cassandra.max.retries=20
connect.cassandra.error.policy=throw
connect.cassandra.contact.points=docker.for.mac.localhost
connect.cassandra.port=9042
connect.cassandra.threadpool.size=10
name=CassandraSinkConnector
connect.cassandra.retry.interval=30000
connect.progress.enabled=true
connect.cassandra.consistency.level=LOCAL_ONE
value.converter=org.apache.kafka.connect.json.JsonConverter
key.converter=org.apache.kafka.connect.json.JsonConverter
value.converter.schemas.enable=false
key.converter.schemas.enable=false